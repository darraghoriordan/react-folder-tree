    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  },
  "dependencies": {
    "jest-environment-jsdom": "29.4.3",
