{
  "name": "react-folder-tree",
  "version": "5.0.3",
  "description": "Cloned from main react-folder-tree to add clickable folder names",
  "main": "dist/react-folder-tree.bundle.js",
  "typings": "index.d.ts",
  "files": [
    "dist/react-folder-tree.bundle.js",
    "dist/style.css",
    "index.d.ts"
  ],
  "author": "Darragh ORiordan",
  "keywords": [
    "react",
    "npm",
    "reactjs",
    "npm-package",
    "folder",
    "recursion",
    "npm-module",
    "folders",
