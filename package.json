    "@types/jest": "29.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
